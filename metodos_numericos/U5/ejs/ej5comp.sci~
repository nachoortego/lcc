function M = mat_it_gauss_seidel(A)
	n = size(A,1)
	N = zeros(n,n)

	for i = 1:n
		N(i,i) = A(i,i)
	end
disp(M)

	M = eye(n,n) - inv(N)*A
endfunction

function M = mat_it_jacobi(A)
	n = size(A,1)
	N = zeros(n,n)

	for i = 1:n
		N(i,i) = A(i,i)
	end

	M = eye(n,n) - inv(N)*A
endfunction

A = [
10 -1 2 0
-1 11 -1 3
2 -1 10 -1
0 3 -1 8]

b = [ 6 25 -11 15]

disp(mat_it_jacobi(A))
